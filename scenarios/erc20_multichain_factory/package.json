{
  "name": "erc20-multi-chain-factory",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean": "rescript clean",
    "build": "rescript",
    "watch": "rescript -w",
    "mocha": "mocha --exit test/**/*.{ts,res.mjs}",
    "test": "pnpm build && pnpm mocha",
    "codegen": "cargo run --manifest-path ../../codegenerator/cli/Cargo.toml -- codegen",
    "dev": "pnpm build && envio dev",
    "start": "pnpm build && envio start"
  },
  "dependencies": {
    "envio": "file:../../codegenerator/cli/npm/envio",
    "ethers": "6.8.0",
    "helpers": "../helpers",
    "mocha": "10.2.0",
    "rescript": "11.1.3",
    "rescript-schema": "9.3.4"
  },
  "optionalDependencies": {
    "generated": "./generated"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}

{
  "name": "erc20-multi-chain-factory",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean": "rescript clean",
    "build": "rescript",
    "watch": "rescript -w",
    "mocha": "NODE_OPTIONS='--no-warnings --import tsx' mocha --timeout 30000 --exit test/**/*.res.mjs",
    "test": "pnpm build && pnpm mocha",
    "codegen": "cargo run --manifest-path ../../codegenerator/cli/Cargo.toml -- codegen",
    "dev": "pnpm build && envio dev",
    "start": "pnpm build && envio start"
  },
  "dependencies": {
    "envio": "file:../../codegenerator/cli/npm/envio",
    "tsx": "4.21.0",
    "helpers": "../helpers",
    "mocha": "10.2.0",
    "rescript": "12.1.0",
    "sury": "11.0.0-alpha.4"
  },
  "optionalDependencies": {
    "generated": "./generated"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}

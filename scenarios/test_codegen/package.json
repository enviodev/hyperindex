{
  "name": "gravatar",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "rescript",
    "watch": "rescript -w",
    "test": "rescript && tsc --noEmit && vitest run",
    "test:update-log-snapshots": "ENVIO_TEST_LOGGING_FORMAT=1 node scripts/update-log-snapshots.mjs",
    "codegen": "envio codegen",
    "docker-up": "cargo run --manifest-path ../../codegenerator/cli/Cargo.toml -- local docker up",
    "docker-down": "cargo run --manifest-path ../../codegenerator/cli/Cargo.toml -- local docker down",
    "db-setup": "cargo run --manifest-path ../../codegenerator/cli/Cargo.toml -- local db-migrate setup",
    "start": "envio start"
  },
  "keywords": [
    "ReScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "^0.9.2",
    "@types/node": "^24.10.1",
    "tsx": "4.21.0",
    "vitest": "^3.0.0",
    "rescript": "11.1.3",
    "rescript-schema": "9.3.4",
    "sinon": "^15.0.4"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "helpers": "workspace:*",
    "typescript": "5.9.3",
    "ts-expect": "1.3.0",
    "viem": "2.21.0",
    "envio": "file:../../codegenerator/cli/npm/envio"
  },
  "optionalDependencies": {
    "generated": "./generated"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}

{
  "name": "gravatar",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "rescript",
    "watch": "rescript -w",
    "mocha": "NODE_OPTIONS='--no-warnings --import tsx' mocha --timeout 30000 --exit 'test/**/*.{ts,res.mjs}' --ignore 'test/fixtures/**'",
    "test": "rescript && tsc --noEmit && pnpm mocha",
    "test-coverage": "rescript && nyc mocha",
    "test:update-log-snapshots": "ENVIO_TEST_LOGGING_FORMAT=1 node scripts/update-log-snapshots.mjs",
    "codegen": "envio codegen",
    "docker-up": "cargo run --manifest-path ../../packages/cli/Cargo.toml -- local docker up",
    "docker-down": "cargo run --manifest-path ../../packages/cli/Cargo.toml -- local docker down",
    "db-setup": "cargo run --manifest-path ../../packages/cli/Cargo.toml -- local db-migrate setup",
    "start": "envio start"
  },
  "keywords": [
    "ReScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "^0.9.2",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^24.10.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "ethers": "^6.4.0",
    "tsx": "4.21.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "rescript": "11.1.3",
    "rescript-schema": "9.3.4",
    "sinon": "^15.0.4"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "helpers": "workspace:*",
    "rescript-nodejs": "16.0.0",
    "typescript": "5.9.3",
    "ts-expect": "1.3.0",
    "viem": "2.21.0",
    "envio": "file:../../packages/envio"
  },
  "optionalDependencies": {
    "generated": "./generated"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}

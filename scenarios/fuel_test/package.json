{
  "name": "fuel_test",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "ts:clean": "tsc --clean",
    "ts:build": "tsc --build",
    "ts:watch": "tsc --watch",
    "ts:test": "tsc --noEmit",
    "res:clean": "rescript clean",
    "res:build": "rescript",
    "res:watch": "rescript -w",
    "mocha": "NODE_OPTIONS='--experimental-specifier-resolution=node --no-warnings' mocha --loader=ts-node/esm test/**/*.{ts,res.mjs}",
    "codegen": "cargo run --manifest-path ../../codegenerator/cli/Cargo.toml -- codegen",
    "dev": "cargo run --manifest-path ../../codegenerator/cli/Cargo.toml -- dev",
    "test": "rescript && pnpm mocha",
    "start": "envio start"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "10.0.6",
    "@types/node": "20.8.8",
    "typescript": "5.9.3",
    "ts-node": "10.9.2",
    "mocha": "10.2.0"
  },
  "dependencies": {
    "chai": "4.3.10",
    "envio": "file:../../codegenerator/cli/npm/envio",
    "rescript-schema": "9.3.4",
    "ts-expect": "1.3.0",
    "rescript": "11.1.3"
  },
  "optionalDependencies": {
    "generated": "./generated"
  },
  "engines": {
    "node": ">=22.0.0 <=22.10.0"
  }
}

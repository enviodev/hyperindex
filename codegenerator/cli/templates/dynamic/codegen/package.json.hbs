{
  "name": "generated",
  "version": "0.1.0",
  "main": "index.js",
  "types": "index.d.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rescript clean",
    "build": "rescript",
    "watch": "rescript -w",
    "format": "rescript format -all",
    "db-up": "node -e 'import(\"./src/db/Migrations.res.mjs\").then(m => m.runUpMigrations(true))'",
    "db-down": "node -e 'import(\"./src/db/Migrations.res.mjs\").then(m => m.runDownMigrations(true))'",
    "db-setup": "node -e 'import(\"./src/db/Migrations.res.mjs\").then(m => m.runUpMigrations(true, true))'",
    "print-benchmark-summary": "node -e 'import(\"./src/Benchmark.res.mjs\").then(m => m.Summary.printSummary())'",
    {{!-- We need this to always have cwd at the root of the project --}}
    "start": "cd {{relative_path_to_root_from_generated}} &&  node --loader {{relative_path_to_generated_from_root}}/node_modules/ts-node/esm.mjs --experimental-specifier-resolution=node {{relative_path_to_generated_from_root}}/src/Index.res.mjs"
  },
  "keywords": [
    "ReScript"
  ],
  "engines": {
    "node": ">=22.0.0 <=22.10.0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    {{#if is_fuel_ecosystem}}
    "@fuel-ts/crypto": "0.96.1",
    "@fuel-ts/errors": "0.96.1",
    "@fuel-ts/hasher": "0.96.1",
    "@fuel-ts/math": "0.96.1",
    "@fuel-ts/utils": "0.96.1",
    "@fuel-ts/address": "0.96.1",
    {{/if}}
    "ts-node": "10.9.1",
    "@rescript/react": "0.12.1",
    "bignumber.js": "9.1.2",
    "date-fns": "3.3.1",
    "dotenv": "16.4.5",
    "ethers": "6.8.0",
    "express": "4.19.2",
    "ink": "3.2.0",
    "ink-big-text": "1.2.0",
    "ink-spinner": "4.0.3",
    "js-sdsl": "4.4.2",
    "pino": "8.16.1",
    "postgres": "3.4.1",
    "react": "18.2.0",
    "rescript": "11.1.3",
    "rescript-envsafe": "5.0.0",
    "rescript-schema": "9.3.0",
    "envio": "{{envio_version}}",
    "viem": "2.21.0",
    "yargs": "17.7.2",
    "prom-client": "15.0.0"
  }
}

{
  "name": "generated",
  "version": "0.1.0",
  "main": "index.js",
  "types": "index.d.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rescript clean",
    "build": "rescript",
    "watch": "rescript -w",
    "format": "rescript format -all",
    {{!-- We need this to always have cwd at the root of the project --}}
    {{!-- For imports and dotenv to correctly resolve --}}
    "db-up": "cd {{relative_path_to_root_from_generated}} && node -e 'import(\"{{relative_path_to_generated_from_root}}/src/db/Migrations.res.mjs\").then(m => m.runUpMigrations(true))'",
    "db-down": "cd {{relative_path_to_root_from_generated}} && node -e 'import(\"{{relative_path_to_generated_from_root}}/src/db/Migrations.res.mjs\").then(m => m.runDownMigrations(true))'",
    "db-setup": "cd {{relative_path_to_root_from_generated}} && node -e 'import(\"{{relative_path_to_generated_from_root}}/src/db/Migrations.res.mjs\").then(m => m.runUpMigrations(true, true))'",
    "print-benchmark-summary": "cd {{relative_path_to_root_from_generated}} && node -e 'import(\"{{relative_path_to_generated_from_root}}/src/Benchmark.res.mjs\").then(m => m.Summary.printSummary())'",
    {{!-- tsx is needed for start script to support TypeScript handlers --}}
    "start": "cd {{relative_path_to_root_from_generated}}{{#if is_typescript}} && tsc --noEmit{{/if}} && node --no-warnings --import {{relative_path_to_generated_from_root}}/node_modules/envio/node_modules/tsx/dist/esm/index.mjs {{relative_path_to_generated_from_root}}/src/Index.res.mjs"
  },
  "keywords": [
    "ReScript"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    {{#if is_fuel_ecosystem}}
    "@fuel-ts/crypto": "0.96.1",
    "@fuel-ts/errors": "0.96.1",
    "@fuel-ts/hasher": "0.96.1",
    "@fuel-ts/math": "0.96.1",
    "@fuel-ts/utils": "0.96.1",
    "@fuel-ts/address": "0.96.1",
    {{/if}}
    "bignumber.js": "9.1.2",
    "date-fns": "3.3.1",
    "ethers": "6.8.0",
    "js-sdsl": "4.4.2",
    "pino": "8.16.1",
    "react": "19.2.0",
    "rescript": "11.1.3",
    "rescript-schema": "9.3.4",
    "envio": "{{envio_version}}",
    "viem": "2.21.0",
    "prom-client": "15.0.0"
  }
}

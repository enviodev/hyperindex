module EntityType = {
  @genType
  type t = 
  {{#each entities as | entity |}}
    | @as("{{entity.name.original}}") {{entity.name.capitalized}}
  {{/each}}
    | @as("dynamic_contract_registry") DynamicContractRegistry

  let name = "ENTITY_TYPE"
  let variants = [
  {{#each entities as | entity |}}
    {{entity.name.capitalized}},
  {{/each}}
    DynamicContractRegistry,
  ]
  let config = Table.makeEnumConfig(~name, ~variants)
}
{{#each gql_enums as | enum |}} 

module {{enum.name.capitalized}} = {
  @genType
  type t = 
  {{#each enum.params as | param | }}
    | @as("{{param.original}}") {{param.capitalized}}
  {{/each}}

  let name = "{{enum.name.capitalized}}"
  let variants = [
  {{#each enum.params as | param | }}
    {{param.capitalized}},
  {{/each}}
  ]
  let config = Table.makeEnumConfig(~name, ~variants)
}
{{/each}}

let allEnums = ([
  EntityType.config->Table.fromGenericEnumConfig,
{{#each gql_enums as | enum |}} 
  {{enum.name.capitalized}}.config->Table.fromGenericEnumConfig,
{{/each}}
])
